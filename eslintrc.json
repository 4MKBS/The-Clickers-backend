{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "no-unused-vars": ["error", {"argsIgnorePattern": "^_"}],
        "no-console": "warn",
        "eqeqeq": ["error", "always"],
        "curly": "error"
    }
}
